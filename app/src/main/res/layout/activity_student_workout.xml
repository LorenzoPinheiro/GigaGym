<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".workout.student.StudentWorkoutActivity">

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:width="343sp"
        android:height="42sp"
        android:backgroundTint="#E8DEF8"
        android:paddingStart="48sp"
        android:paddingEnd="48sp"
        android:text="Finalizar treino"
        android:textAlignment="center"
        android:textColor="#000000"
        app:cornerRadius="12sp"
        app:iconPadding="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageWolf11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/wolf_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.051" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="374sp"
        android:layout_height="48sp"
        android:layout_marginTop="104dp"
        android:background="#FEF7FF"
        android:padding="14sp"
        android:text="Treino Brutal"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="375dp"
        android:layout_height="3sp"
        android:layout_marginBottom="560dp"
        android:background="#65558F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:width="360sp"
        android:height="80sp"
        android:backgroundTint="#FEF7FF"
        android:paddingStart="48sp"
        android:paddingEnd="48sp"
        android:text="Exercicio 1"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        app:cornerRadius="12sp"
        app:iconPadding="8sp"
        app:layout_constraintEnd_toEndOf="@+id/button11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/button11"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="360sp"
        android:height="80sp"
        android:backgroundTint="#FEF7FF"
        android:paddingStart="48sp"
        android:paddingEnd="48sp"
        android:text="Exercicio 2"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        app:cornerRadius="12sp"
        app:iconPadding="8sp"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintEnd_toEndOf="@+id/button12"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/button12"
        app:layout_constraintTop_toBottomOf="@+id/button10" />

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="232dp"
        android:width="360sp"
        android:height="80sp"
        android:backgroundTint="#FEF7FF"
        android:paddingStart="48sp"
        android:paddingEnd="48sp"
        android:text="Exercicio 3"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        app:cornerRadius="12sp"
        app:iconPadding="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:srcCompat="@drawable/icon"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="68dp" />

</androidx.constraintlayout.widget.ConstraintLayout>